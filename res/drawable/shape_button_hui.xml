<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>

            <!-- 填充的颜色 -->
            <solid android:color="#e2e2e2" />
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="5dp" />

            <stroke
                android:width="1dp"
                android:color="#e2e2e2" />
        </shape>
    </item>

</layer-list>